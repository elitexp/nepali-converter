    "date",
    "rupees",
    "money",
    "currency"
  ],
  "homepage": "https://github.com/elitexp/nepali-converter",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sujip Maharjan",
      "email": "elitexp2008@gmail.com"
    }
